<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.librarymanagement2.AddBookController">
   <children>
      <Label layoutX="120.0" layoutY="63.0" text="THIS IS ADD SCENE">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <TextField fx:id="ISBN" layoutX="201.0" layoutY="152.0" promptText="ISBN" />
      <TextField fx:id="title" layoutX="201.0" layoutY="208.0" promptText="Title" />
      <TextField fx:id="author" layoutX="201.0" layoutY="266.0" promptText="Author" />
      <TextField fx:id="publishcationDate" layoutX="201.0" layoutY="319.0" promptText="Publication Date" />
      <TextField fx:id="publisher" layoutX="201.0" layoutY="371.0" promptText="Publisher" />
      <Button fx:id="addBookButton" layoutX="256.0" layoutY="543.0" mnemonicParsing="false" onMouseClicked="#addBook" text="Add" />
      <TextField fx:id="image_url" layoutX="201.0" layoutY="423.0" promptText="Image Link" />
      <TextField fx:id="noOfCopies" layoutX="201.0" layoutY="465.0" promptText="noOfCopies" />
      <TextField fx:id="price" layoutX="201.0" layoutY="504.0" promptText="Price" />
   </children>
</AnchorPane>
